{
  "name": "slack",
  "version": "1.0.13",
  "private": true,
  "description": "Notify Slack of GitHub Actions job and step status.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/act10ns/slack.git"
  },
  "keywords": [
    "actions",
    "slack",
    "notify"
  ],
  "author": "satterly",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^3.0.0",
    "@octokit/webhooks": "^7.15.1",
    "@slack/webhook": "^5.0.3",
    "flow-bin": "^0.138.0",
    "graphql": "^14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.13",
    "@typescript-eslint/parser": "^2.0.0",
    "@zeit/ncc": "^0.22.3",
    "axios-mock-adapter": "^1.18.1",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^24.9.0",
    "jest-circus": "^26.0.1",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.5"
  }
}
